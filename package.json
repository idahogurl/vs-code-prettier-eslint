{
	"name": "prettier-eslint",
	"displayName": "Prettier Eslint Formatter",
	"description": "Extension to format JavaScript code using prettier-eslint package",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Formatters"
	],
	"main": "extension.js",
	"activationEvents": [
		"onLanguage:javascript"
	],
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.20.1",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"eslint": "^6.8.0",
		"prettier": "^1.19.1",
		"prettier-eslint": "^9.0.1"
	}
}
